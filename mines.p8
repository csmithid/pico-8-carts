pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

mapdim = 14
difficulty = 22
--
cursor={
  x=1,
  y=1,
  color=8,
  draw = function(self)
    rect((self.x*8)-1,(self.y*8)-1,(self.x*8)+8,(self.y*8)+8,self.color)
  end,
  move = function(self,dx,dy)
    if self.x+dx >= 1 and self.x+dx <= mapdim then
      self.x += dx
    end
    if self.y+dy >= 1 and self.y+dy <= mapdim then
      self.y += dy
    end
  end
}

field={
  make = function(self)
    --fill with empty tiles
    for i=1,mapdim do
      self[i]={}
      for j=1,mapdim do
        self[i][j]=new_tile()
      end
    end
    --place mines
    mines = 0
    while mines < difficulty do
      t = random_tile()
      if not t.mine then
        mines+=1
        t.mine=true
        t.punched_sprite=11
      end
    end
    --set punch numbers
    for i=1,mapdim do
      for j=1,mapdim do
        field[i][j]:set_punch(i,j)
      end
    end
  end,
  draw = function(self)
    for i=1,mapdim do
      for j=1,mapdim do
        self[i][j]:draw(i,j)
      end
    end
  end
}

function new_tile()
  tile={
    sprite=1,
    punched_sprite=2,
    mine=false,
    punched=false,
    flagged=false,
    draw = function(self,i,j)
      if self.punched then
        spr(self.punched_sprite,i*8,j*8)
      elseif self.flagged then
        spr(12,i*8,j*8)
      else
        spr(1,i*8,j*8)
      end
    end,
    set_punch = function(self,x,y)
      count=0
      for i=-1,1 do
        for j=-1,1 do
          if x+i > 0 and x+i < mapdim and y+j > 0 and y+j < mapdim then
            if field[x+i][y+j].mine then count +=1 end
            if i==0 and j==0 then count-=1 end
          end
        end
      end
      self.punched_sprite=2+count
    end
  }
	return tile
end

function _init()
  field:make()
end

function _update()
  keys()
end

function _draw()
  cls()
  field:draw()
  cursor:draw()
end

function random_tile()
  return field[flr(rnd(mapdim))+1][flr(rnd(mapdim))+1]
end

function keys()
  if btnp(0) then cursor:move(-1,0) end
  if btnp(1) then cursor:move(1,0) end
  if btnp(2) then cursor:move(0,-1) end
  if btnp(3) then cursor:move(0,1) end
  if btnp(4) then field[cursor.x][cursor.y].punched=true end
  if btnp(5) then field[cursor.x][cursor.y].flagged=true end
end

