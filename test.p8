pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
 ship_x = 63
 ship_y = 63
 ship_angle = 0.5
 ship_vel = 0
end

function _update()
 if (btn(0)) ship_angle -= 0.01
 if (btn(1)) ship_angle += 0.01
 
 if btn(2) then
  ship_x += sin(ship_angle)
  ship_y += cos(ship_angle)
 end
end

function _draw()
 cls()
 
 -- instructions
 print("press ‹ ‘ to turn", 10, 10, 15)
 print("press ” to go forward", 10, 16, 15)
 
 -- call rotated sprite function
 -- for the ship
 rotsprite(16, ship_x, ship_y, ship_angle)
 
 -- dog with a few layers
 layeredsprite(32, ship_x - 12, ship_y, ship_angle, 4)
 
 -- for a many layered giraffe
 -- use layered function
 layeredsprite(1, ship_x + 12, ship_y, ship_angle, 15)
end

function layeredsprite(sprite, posx, posy, angle, layers)
 for layer = 1, layers do
  rotsprite(sprite, posx, posy, angle)
  sprite += 1
  posy -= 1
 end
end

function rotsprite(sprite, posx, posy, angle)
 -- get x0, y0 coordinates for center of sprite
 local x0 = sprite % 16 * 8 + 4
 local y0 = flr(sprite / 16) * 8 + 4
 
 -- calculate sin/cos only once
 local s = sin(angle)
 local c = cos(angle) 
 for x = -5, 5 do
  for y = -5, 5 do
   -- calculate rotated xr, yr values
   -- basic 2d rotation matrix maths
   local xr = c * x - s * y
   local yr = s * x + c * y
   if xr < 4
   and xr >= -4
   and yr < 4
   and yr >= -4 then
    -- get color at position
    local col = sget(x0 + xr, y0 + yr)
    -- draw color at non-rotated coordinates
    if (col ~= 0) pset(posx + x, posy + y, col)
   end
  end
 end
end
