pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--checkerboard mini-doodle
--by trasevol_dog


function _init()
 t=0
 
 _draw()
 flip()
 extcmd("rec")
 for i=1,100 do
  _update()
  _draw()
  flip()
 end
 extcmd("video")
end

function _update()
 t+=0.03
end

function _draw()
palt(0,false)

 local ca,cb=0,7
 
 if t%3<1.5 then
  ca,cb=cb,ca
 end
 
 cls(ca)
 
 if t%1.5<0.5 then
 
  make_sprite(ca,cb,1-t%1.5*1.5)
  
  local b=false
  for x=0,127,16 do
  for y=0,127,16 do
   if b then
   spr(0,x,y,2,2)
   end
   
   b=not b
  end
  b=not b
  end
 
 elseif t%1.5<1 then
  
  local kt=(t%1.5-0.5)*2
  local kt=-sin(kt/4)*16
  
  local b=false
  for x=-16,128,16 do
  for y=-16,128,16 do
   local b=(x+y)%32>=16
   
   --local a=(flr(t/3)-flr((t/1.5)%2))*0.25
   local a=(flr(t/1.5))%2*0.25
   local dx,dy=cos(a),sin(a)
   
   if b then
    spr(0,x+dx*kt,y+dy*kt,2,2)
   end
  end
  end
  
 else
 
  make_sprite(ca,cb,0.25+(t%1.5-1)*1.5)
  
  local b=true
  for x=0,127,16 do
  for y=0,127,16 do
   if b then
   spr(0,x,y,2,2)
   end
   
   b=not b
  end
  b=not b
  end
 
 end
end

function make_sprite(ca,cb,f)
 local r=11*f
 for x=0,15 do
 for y=0,15 do
  local d=sqr(x-7.5)+sqr(y-7.5)
  if d<sqr(r) then
   sset(x,y,cb)
  else
   sset(x,y,ca)
  end
 end
 end
end

function lerp(a,b,i) return (1-i)*a+i*b end
function sqr(a) return a*a end

