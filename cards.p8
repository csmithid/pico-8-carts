pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
cw = 10
ch = 14

gravity=.5
elasticity=.75
friction = 0.99
timer=0

suit_colors={8,12,9,11}

cards={}

function _init()
	rectfill(0,0,128,128,3)
end

function _update60()
	timer+=1
	if timer%2==0 then add(cards,new_card(flr(rnd(4))+1,flr(rnd(9))+1,flr(rnd(128-cw))+(cw/2),flr(rnd(64))-64)) end
	for card in all(cards) do
	 card:update()
	end

end

function _draw()
	for card in all(cards) do
		card:draw()
	end
end

function new_card(suit,value,x,y)
	local card = {}
	card.suit = suit
	card.value = value
	card.x = x
	card.y = y
	card.dx = pick({3,-3})
	card.dy = 0
	card.col=suit_colors[card.suit]
	card.bounced=false
	card.draw = function(self)
		rect(self.x+1,self.y,self.x+cw-1,self.y+ch,0)
		rect(self.x,self.y+1,self.x+cw,self.y+ch-1,0)
		rectfill(self.x+1,self.y+1,self.x+cw-1,self.y+ch-1,7)
		print(value,self.x+2,self.y+2,self.col)
		spr(self.suit,self.x+6,self.y+2)
	end
	card.update = function(self)
		self.x+=self.dx
		self.y+=self.dy

		if self.y+ch>128 then
			self.dy=(-self.dy*elasticity)
			self.y=128-ch
		end
		self.dy+=gravity

		if self.x<0 or self.x>128-(cw) then
		 	if self.bounced then
				del(cards,self)
			else
				self.dx = -self.dx
				self.bounced=true
			end
		end
		--actuate



	end
	return card
end

--tweens

function tween(target, property, destination, duration, func)
  local tween_info = {
    target = target,
    property = property,
    base_value = target[property],
    change = destination - target[property],
    duration = duration,
    elapsed = 0,
    func = func
  }
  add(tweens,tween_info)
end

function tween_update()
  for t in all(tweens) do
    if t.elapsed > t.duration then
      t.target[t.property] = t.base_value + t.change
      del(tweens, t)
    else
      t.elapsed += 1
      t.target[t.property] = t.func(
          t.elapsed,
          t.base_value,
          t.change,
          t.duration)
    end
  end
end

function pick(table)
	return table[flr(rnd(#table))+1]
end
