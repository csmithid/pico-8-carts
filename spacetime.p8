pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

ship = {
  x=64,
  y=64,
  dx=0,
  dy=0,
  sp=1,
  fr = 0.9,
  speed = 3,
  draw = function(self)
    spr(self.sp,self.x,self.y)
  end,
  update = function(self)
    self.x+=self.dx,
    self.y+=self.dy,
    self.dx*=self.fr,
    self.dy*=self.fr,
    if abs(self.dx) < 0.25 then self.dx = 0 end
    if abs(self.dy) < 0.25 then self.dy = 0 end
    particles:new(self.x+4,self.y+7,8,rnd(10)+5,.75,0.9)
  end
}

function _update()
  handle_keys()
  if (abs(ship.dx)>0) or (abs(ship.dy)>0) then
    particles:new(rnd(128),0,7,rnd(10)+20,.75,0.9)
    particles:update()
    ship:update()
  end
end

function _draw()
  cls()
  ship:draw()
  particles:draw()
end

function handle_keys()
  if btn(0) then ship.dx = -ship.speed end
  if btn(1) then ship.dx = ship.speed end
  if btn(2) then ship.dy = -ship.speed end
  if btn(3) then ship.dy = ship.speed end
end

particles={ --generic particle system (self,x,y,dx,dy,ddx,ddy,c,dc,r,dr,l)
  new = function(self,x,y,col,speed,angle,friction)
    particle={
      x=x,
      y=y,
      dx=speed*cos(angle),
      dy=speed*sin(angle),
      col=col,
      fric = friction,
      life=30,
      update = function(self)
        self.dx*=self.fric
        self.dy*=self.fric

        self.x+=self.dx
        self.y+=self.dy

        self.life-=1
      end,
      draw = function(self)
        pset(self.x,self.y,self.col)
      end
    }
    add(self,particle)
  end,
  update = function(self)
    for p in all(self) do
      if p.life <= 0 then del(self,p) else
        p:update()
      end
    end
  end,
  draw = function(self)
    for p in all(self) do p:draw() end
  end
}
