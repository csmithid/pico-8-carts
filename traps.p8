pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
map_width=16
map_height=16
pointer={x=2,y=2}

function _init()
	tiles=new_map(4)
end

function _update()
if btnp(0) then pointer.x-=1 end
if btnp(1) then pointer.x+=1 end
if btnp(2) then pointer.y-=1 end
if btnp(3) then pointer.y+=1 end
end
function _draw()
cls()
for i=0,15 do
	for j=0,15 do
		spr(1,i*8,j*8)
	end
end
spr(2,pointer.x*8,pointer.y*8)
end

function new_map(difficulty)
	local tiles={}
	for i=1,map_width do
		for j=1,map_height do
			tile={sp=1,x=i,y=j}
			tiles[i][j]=tile
		end
	end
	return tile
end
